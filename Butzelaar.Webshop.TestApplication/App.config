    <contexts>
      <context type="Butzelaar.Webshop.Database.WebshopContext, Butzelaar.Webshop.Database">
        <databaseInitializer type="Butzelaar.Webshop.Database.WebshopContextInitializer, Butzelaar.Webshop.Database" />
      </context>
    </contexts>
